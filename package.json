        "@webcomponents/custom-elements": "1.4.1",
        "angular-calendar": "0.28.13",
        "angular-draggable-droppable": "4.4.4",
        "angular-resizable-element": "3.3.0",
        "angularx-flatpickr": "6.3.1",
        "bootstrap": "4.4.1",
        "calendar-utils": "0.8.0",
        "core-js": "3.6.5",
        "date-fns": "2.13.0",
        "es6-shim": "0.35.5",
        "flag-icon-css": "3.4.6",
