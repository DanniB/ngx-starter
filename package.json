{
  "name": "starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "doc:build": "compodoc -p src/tsconfig.app.json --theme 'material'",
    "doc:serve": "compodoc -s",
    "doc:buildandserve": "compodoc -p src/tsconfig.app.json --theme 'material' -s",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "lint": "tslint --fix --project src/tsconfig.app.json",
    "lint:check": "tslint-config-prettier-check ./tslint.json",
    "ng": "ng",
    "start": "ng serve --open",
    "stylelint": "stylelint --fix '**/*.scss' --syntax scss",
    "test": "ng test",
    "test:all": "concurrently --kill-others-on-fail 'npm run lint' 'npm run stylelint' 'npm run test'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/animations": "6.0.2",
    "@angular/cdk": "6.0.2",
