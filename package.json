  "devDependencies": {
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.2",
    "@angular/language-service": "6.0.2",
    "@compodoc/compodoc": "1.1.3",
    "@types/jasmine": "2.8.7",
    "@types/node": "10.1.2",
    "codelyzer": "4.3.0",
    "concurrently": "3.5.1",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
