{
    "name": "starter",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "ng --progress=false build --stats-json",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json",
        "doc:build": "compodoc -p src/tsconfig.app.json --theme 'material'",
        "doc:serve": "compodoc -s",
        "doc:buildandserve": "compodoc -p src/tsconfig.app.json --theme 'material' -s",
        "e2e": "ng --progress=false e2e",
        "eslint": "eslint 'src/**/*.ts' --fix",
        "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check",
        "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss,.html}\"",
        "format:fix": "prettier --config ./.prettierrc --write \"src/**/*{js,json,css,md,ts,html,component.html}\"",
        "karma": "karma start --single-run",
        "modernizr": "modernizr -uc modernizr-config.json -d modernizr-custom.min.js",
        "ng": "ng",
        "pretty-quick": "pretty-quick",
        "start": "ng serve --open",
        "stylelint": "stylelint --fix '**/*.scss'",
        "stylelint:check": "stylelint-config-prettier-check",
        "test": "ng test --watch=false --browsers --progress=false --source-map=false FirefoxHeadless",
        "testAll": "npm run tslint && npm run stylelint && npm run test"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "9.0.0-rc.1",
        "@angular/cdk": "9.0.0-rc.0",
        "@angular/common": "9.0.0-rc.1",
        "@angular/compiler": "9.0.0-rc.1",
        "@angular/core": "9.0.0-rc.1",
        "@angular/flex-layout": "8.0.0-beta.27",
        "@angular/forms": "9.0.0-rc.1",
        "@angular/material": "9.0.0-rc.0",
        "@angular/platform-browser": "9.0.0-rc.1",
        "@angular/platform-browser-dynamic": "9.0.0-rc.1",
        "@angular/platform-server": "9.0.0-rc.1",
        "@angular/router": "9.0.0-rc.1",
        "@angular/upgrade": "9.0.0-rc.1",
        "@clr/angular": "2.3.1",
        "@clr/icons": "2.3.1",
        "@clr/ui": "2.3.1",
        "@covalent/core": "2.1.1",
        "@fortawesome/angular-fontawesome": "0.5.0",
        "@fortawesome/fontawesome-svg-core": "1.2.25",
        "@fortawesome/free-brands-svg-icons": "5.11.2",
        "@fortawesome/free-regular-svg-icons": "5.11.2",
        "@fortawesome/free-solid-svg-icons": "5.11.2",
        "@ng-bootstrap/ng-bootstrap": "5.1.4",
        "@ng-select/ng-select": "3.6.0",
        "@ngx-translate/core": "11.0.1",
        "@webcomponents/custom-elements": "1.3.0",
        "angular-calendar": "0.28.0-beta.4",
        "angular-draggable-droppable": "4.3.8",
        "angular-resizable-element": "3.2.6",
        "angularx-flatpickr": "6.1.1",
        "bootstrap": "4.3.1",
        "calendar-utils": "0.7.0",
        "core-js": "3.4.0",
        "date-fns": "2.7.0",
        "es6-shim": "0.35.5",
        "flag-icon-css": "3.4.5",
        "flatpickr": "4.6.3",
        "hammerjs": "2.0.8",
