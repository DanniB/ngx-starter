{
  "name": "starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "doc:build": "compodoc -p src/tsconfig.app.json --theme 'material'",
    "doc:serve": "compodoc -s",
    "doc:buildandserve": "compodoc -p src/tsconfig.app.json --theme 'material' -s",
    "e2e": "ng e2e",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "lint": "tslint --fix --project src/tsconfig.app.json",
    "lint:check": "tslint-config-prettier-check ./tslint.json",
    "modernizr": "modernizr -uc modernizr-config.json -d modernizr-custom.min.js",
    "ng": "ng",
    "start": "ng serve --open",
    "stylelint": "stylelint --fix '**/*.scss' --syntax scss",
    "test": "ng test",
    "test:all": "npm run lint && npm run stylelint && npm run test'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.4",
    "@angular/cdk": "7.0.4",
    "@angular/common": "7.0.4",
    "@angular/compiler": "7.0.4",
    "@angular/core": "7.0.4",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.0.4",
    "@angular/material": "7.0.4",
    "@angular/platform-browser": "7.0.4",
    "@angular/platform-browser-dynamic": "7.0.4",
    "@angular/platform-server": "7.0.4",
    "@angular/router": "7.0.4",
    "@angular/upgrade": "7.0.4",
    "@covalent/core": "2.0.0-beta.1",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-brands": "5.0.13",
    "@fortawesome/fontawesome-free-regular": "5.0.13",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@webcomponents/custom-elements": "1.2.1",
    "bootstrap": "4.1.3",
    "clarity-angular": "0.10.28",
    "clarity-icons": "0.10.28",
    "clarity-ui": "0.10.28",
    "core-js": "2.5.7",
    "es6-shim": "0.35.4",
    "flag-icon-css": "3.2.1",
    "jquery": "3.3.1",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "mutationobserver-shim": "0.3.2",
    "ngx-bootstrap": "3.1.1",
    "ngx-logger": "3.3.6",
    "primeng": "7.0.0-beta.1",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.6",
    "@angular-devkit/core": "7.0.6",
    "@angular/cli": "7.0.6",
    "@angular/compiler-cli": "7.0.4",
    "@angular/language-service": "7.0.4",
    "@compodoc/compodoc": "1.1.6",
    "@types/jasmine": "2.8.11",
    "@types/node": "10.12.9",
    "codelyzer": "4.5.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "jshint-stylish": "2.2.1",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-firefox-launcher": "1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "2.0.0",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-remap-istanbul": "0.6.0",
    "modernizr": "3.6.0",
    "node-sass": "4.10.0",
    "prettier": "1.15.2",
    "protractor": "5.4.1",
    "stylelint": "9.8.0",
    "stylelint-config-prettier": "4.0.0",
    "stylelint-config-standard": "18.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.16.0",
    "typescript": "3.1.6",
    "webpack-bundle-analyzer": "3.0.3"
  }
