{
    "name": "starter",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "ng --progress=false build --stats-json",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json",
        "doc:build": "compodoc -p src/tsconfig.app.json --theme 'material'",
        "doc:serve": "compodoc -s",
        "doc:buildandserve": "compodoc -p src/tsconfig.app.json --theme 'material' -s",
        "e2e": "ng --progress=false e2e",
        "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss,.html}\"",
        "format:fix": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.js,.json,.css,.scss,.html}\"",
        "karma": "karma start --single-run",
        "modernizr": "modernizr -uc modernizr-config.json -d modernizr-custom.min.js",
        "ng": "ng",
        "start": "ng serve --open",
        "stylelint": "stylelint --fix '**/*.scss'",
        "stylelint:check": "stylelint-config-prettier-check",
        "test": "ng test --watch=false --browsers --progress=false --source-map=false FirefoxHeadless",
        "tslint": "tslint --fix --project src/tsconfig.app.json",
        "tslint:check": "tslint-config-prettier-check ./tslint.json",
        "testAll": "npm run tslint && npm run stylelint && npm run test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "7.2.4",
        "@angular/cdk": "7.3.1",
        "@angular/common": "7.2.4",
        "@angular/compiler": "7.2.4",
        "@angular/core": "7.2.4",
        "@angular/flex-layout": "7.0.0-beta.23",
        "@angular/forms": "7.2.4",
        "@angular/material": "7.3.1",
        "@angular/platform-browser": "7.2.4",
        "@angular/platform-browser-dynamic": "7.2.4",
        "@angular/platform-server": "7.2.4",
        "@angular/router": "7.2.4",
        "@angular/upgrade": "7.2.4",
        "@clr/angular": "1.0.5",
        "@clr/icons": "1.0.5",
        "@clr/ui": "1.0.5",
        "@covalent/core": "2.0.1",
        "@fortawesome/fontawesome": "1.1.8",
        "@fortawesome/fontawesome-free-brands": "5.0.13",
        "@fortawesome/fontawesome-free-regular": "5.0.13",
        "@fortawesome/fontawesome-free-solid": "5.0.13",
        "@fortawesome/fontawesome-free-webfonts": "1.0.9",
        "@ng-bootstrap/ng-bootstrap": "4.0.2",
