{
    "name": "starter",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "ng --progress=false build --stats-json",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json",
        "doc:build": "compodoc -p src/tsconfig.app.json --theme 'material'",
        "doc:serve": "compodoc -s",
        "doc:buildandserve": "compodoc -p src/tsconfig.app.json --theme 'material' -s",
        "e2e": "ng --progress=false e2e",
        "eslint": "eslint 'src/**/*.ts' --fix",
        "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check",
        "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss,.html}\"",
        "format:fix": "prettier --config ./.prettierrc --write \"src/**/*{js,json,css,md,ts,html,component.html}\"",
        "karma": "karma start --single-run",
        "modernizr": "modernizr -uc modernizr-config.json -d modernizr-custom.min.js",
        "ng": "ng",
        "pretty-quick": "pretty-quick",
        "start": "ng serve --open",
        "stylelint": "stylelint --fix '**/*.scss'",
        "stylelint:check": "stylelint-config-prettier-check",
        "test": "ng test --watch=false --browsers --progress=false --source-map=false FirefoxHeadless",
        "testAll": "npm run tslint && npm run stylelint && npm run test"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "9.1.4",
        "@angular/cdk": "9.2.2",
        "@angular/common": "9.1.4",
        "@angular/compiler": "9.1.4",
        "@angular/core": "9.1.4",
        "@angular/flex-layout": "9.0.0-beta.29",
        "@angular/forms": "9.1.4",
        "@angular/localize": "9.1.4",
        "@angular/material": "9.2.2",
        "@angular/platform-browser": "9.1.4",
        "@angular/platform-browser-dynamic": "9.1.4",
        "@angular/platform-server": "9.1.4",
        "@angular/router": "9.1.4",
        "@angular/upgrade": "9.1.4",
        "@clr/angular": "3.1.1",
        "@clr/core": "3.1.1",
        "@clr/icons": "3.1.1",
        "@clr/ui": "3.1.1",
        "@covalent/core": "3.0.1",
        "@fortawesome/angular-fontawesome": "0.6.1",
        "@fortawesome/fontawesome-svg-core": "1.2.28",
        "@fortawesome/free-brands-svg-icons": "5.13.0",
        "@fortawesome/free-regular-svg-icons": "5.13.0",
        "@fortawesome/free-solid-svg-icons": "5.13.0",
