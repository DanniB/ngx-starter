    "jquery": "3.3.1",
    "lodash": "4.17.10",
    "moment": "2.22.1",
    "mutationobserver-shim": "0.3.2",
    "ngx-bootstrap": "3.0.0",
    "ngx-logger": "3.0.0-rc1",
    "primeng": "6.0.0-alpha.1",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.1.0",
    "zone.js": "0.8.26"
